<%- include("partials/header") -%>

<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input class="form-control" type="text" name="postTitle" />
    <label>Post</label>
    <textarea
      class="form-control"
      name="postBody"
      cols="5"
      rows="15"
    ></textarea>
    <button class="btn btn-primary" type="submit" name="postBody">
      Publish
    </button>
  </div>
</form>

<%- include("partials/footer") -%>
